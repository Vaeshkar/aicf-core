{
  "name": "aicf-core",
  "version": "2.1.1",
  "description": "Universal AI Context Format (AICF) - Enterprise-grade AI memory infrastructure with 95.5% compression and zero semantic loss",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/aicf-api.d.ts",
      "import": "./dist/aicf-api.js"
    },
    "./reader": {
      "types": "./dist/aicf-reader.d.ts",
      "import": "./dist/aicf-reader.js"
    },
    "./writer": {
      "types": "./dist/aicf-writer.d.ts",
      "import": "./dist/aicf-writer.js"
    },
    "./secure": {
      "types": "./dist/aicf-secure.d.ts",
      "import": "./dist/aicf-secure.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:fix-extensions",
    "build:test": "tsc -p tsconfig.test.json && npm run build:fix-extensions",
    "build:fix-extensions": "node scripts/fix-extensions.js",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "pretest": "npm run build:test",
    "test": "node --test dist/tests/**/*.test.js",
    "test:watch": "node --test --watch dist/**/*.test.js",
    "test:coverage": "c8 node --test dist/**/*.test.js",
    "test:unit": "node --test dist/tests/unit/**/*.test.js",
    "test:integration": "node --test dist/tests/integration/**/*.test.js",
    "test:security": "node --test dist/tests/security/**/*.test.js",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "validate": "npm run typecheck && npm run lint && npm run format:check"
  },
  "keywords": [
    "aicf",
    "ai-context",
    "ai-memory",
    "universal-ai-format",
    "ai-infrastructure",
    "compression",
    "semantic-preservation",
    "cross-platform",
    "langchain",
    "openai",
    "claude",
    "context-management",
    "enterprise-ai",
    "memory-persistence",
    "conversation-format",
    "ai-standard"
  ],
  "author": "Dennis van Leeuwen <dennis@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vaeshkar/aicf-core.git"
  },
  "bugs": {
    "url": "https://github.com/Vaeshkar/aicf-core/issues"
  },
  "homepage": "https://github.com/Vaeshkar/aicf-core#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "c8": "^9.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "better-sqlite3": "^12.0.0"
  },
  "files": [
    "dist/",
    "docs/",
    "examples/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
