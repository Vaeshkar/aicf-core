{
  "name": "aicf-core",
  "version": "1.0.0",
  "description": "Universal AI Context Format (AICF) - Enterprise-grade AI memory infrastructure with 95.5% compression and zero semantic loss",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./types/index.d.ts"
    },
    "./api": {
      "require": "./lib/aicf-api.js",
      "import": "./lib/aicf-api.mjs"
    },
    "./reader": {
      "require": "./lib/aicf-reader.js",
      "import": "./lib/aicf-reader.mjs"
    },
    "./writer": {
      "require": "./lib/aicf-writer.js",
      "import": "./lib/aicf-writer.mjs"
    },
    "./agents": {
      "require": "./lib/agents/index.js",
      "import": "./lib/agents/index.mjs"
    }
  },
  "types": "types/index.d.ts",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:efficiency": "node tests/efficiency-test.js",
    "test:compression": "node tests/compression-test.js",
    "test:roundtrip": "jest tests/roundtrip",
    "test:performance": "jest tests/performance",
    "lint": "eslint src/ --ext .js",
    "format": "prettier --write src/**/*.js",
    "docs": "jsdoc src/ -d docs/api",
    "benchmark": "node tests/benchmark.js",
    "validate": "node scripts/validate-aicf.js"
  },
  "keywords": [
    "aicf",
    "ai-context",
    "ai-memory",
    "universal-ai-format",
    "ai-infrastructure",
    "compression",
    "semantic-preservation",
    "cross-platform",
    "langchain",
    "openai",
    "claude",
    "context-management",
    "enterprise-ai",
    "memory-persistence",
    "conversation-format",
    "ai-standard"
  ],
  "author": "Dennis van Leeuwen <dennis@example.com>",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vaeshkar/aicf-core.git"
  },
  "bugs": {
    "url": "https://github.com/Vaeshkar/aicf-core/issues"
  },
  "homepage": "https://github.com/Vaeshkar/aicf-core#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "chai": "^6.2.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.0",
    "mocha": "^11.7.4",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "better-sqlite3": "^12.0.0"
  },
  "files": [
    "lib/",
    "types/",
    "docs/",
    "examples/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
